@model List<EntityLayer.Concrete.Category>
@{
    Layout = null;
}

    <table>
        <tr>
            <th>ID</th>
            <th>Kategori Adı</th>
            <th>Durumu</th>
            <th>Detaylar</th>
            <th>Sil</th>
        </tr>  
       
            @foreach (var ctr in Model)
            {
        <tr>
            <th>@ctr.CategoryID</th>
            <th>@ctr.CategoryNAME</th>
            <th>@ctr.CategoryStatus</th>
            <th><a href="#">Detaylar</a></th>
            <th><a hreg="#">Sil</a></th>
        </tr>
            }
        
    </table>
